<?xml version="1.0" encoding="windows-1252" ?>
<adfc-config xmlns="http://xmlns.oracle.com/adf/controller" version="1.2">
  <task-flow-definition id="emp-ce-task-flow-definition">
    <default-activity>empce</default-activity>
    <transaction>
      <requires-transaction/>
    </transaction>
    <data-control-scope>
      <shared/>
    </data-control-scope>
    <managed-bean id="__1">
      <managed-bean-name>ContextualEventEHandlerBean</managed-bean-name>
      <managed-bean-class>com.packtpub.adfguide.ch9.view.managed.ContextualEventEHandlerBean</managed-bean-class>
      <managed-bean-scope>view</managed-bean-scope>
    </managed-bean>
    <view id="empce">
      <page>/contextualevent/empce.jsff</page>
    </view>
    <use-page-fragments/>
  </task-flow-definition>
</adfc-config>
